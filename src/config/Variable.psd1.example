@{
	# Gebruikersspecifieke instellingen die vaak aangepast worden (Voorbeeld configuratie - kopieer naar Variable.psd1 en pas aan)

	# VPN config (Voorbeeld)
	port = 443 # Poort voor VPN verbinding
	protocol = 'TCP' # Protocol voor VPN (TCP of UDP)
	vpnSubnet = '10.8.0.0' # Subnet voor VPN netwerk
	vpnMask = '255.255.255.0' # Subnet mask voor VPN netwerk
	testIP = '10.8.0.1' # Test IP adres binnen VPN subnet
	dns1 = '8.8.8.8' # Primaire DNS server
	dns2 = '8.8.4.4' # Secundaire DNS server
	
	# WireGuard config
	wireGuardDefaultDns = '8.8.8.8' # Standaard DNS voor WireGuard
	wireGuardPort       = 51820 # Poort voor WireGuard
	wireGuardBaseSubnet = '10.13.13' # Basis subnet voor WireGuard
	
	# Server configuratie (stel vooraf in om vragen te vermijden - voorbeeld)
	serverName = 'vpn-server'  # Verplicht: Naam van de VPN server
	serverIP = 'jouw.server.ip.hier'  # Verplicht: LAN IP van de server
	serverWanIP = 'jouw.wan.ip.hier'  # Verplicht: WAN IP of DDNS van de server
	lanSubnet = '192.168.1.0' # Verplicht: LAN subnet van het netwerk achter de VPN server
	lanMask = '255.255.255.0' # Verplicht: Subnet mask van het LAN netwerk
	noPass = $true  # Of certificaten zonder wachtwoord gemaakt moeten worden
	remoteClientIP = "jouw.client.ip.hier" # Verplicht: IP van de client

	# client configuratie (Voorbeeld)
	clientName = 'client1'  # Verplicht: Naam van de VPN client
	clientCSVPath = "output\test_clients.csv" # Pad naar CSV bestand met extra clients
}